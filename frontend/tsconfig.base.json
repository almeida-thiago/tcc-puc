{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "./out",
    "rootDir": "./src",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@models/*": [
        "./models/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@enums/*": [
        "./enums/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@store/*": [
        "./store/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
